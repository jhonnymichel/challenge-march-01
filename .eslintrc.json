{
  "extends": "google",
  "env":{
    "browser": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "rules": {
    "no-trailing-spaces": "error",
    "linebreak-style": "off",
    "object-curly-spacing": [1, "always"],
    "arrow-parens": "off",
    "require-jsdoc": "off"
  }
}